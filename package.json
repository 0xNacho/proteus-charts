{
  "name": "proteic",
  "version": "0.0.25",
  "description": "Modern visualization library, ready for batch and streaming data",
  "homepage": "http://proteic.js.org/",
  "main": "dist/proteic.node.js",
  "browser": "dist/proteic.js",
  "module": "index",
  "jsnext:main": "index",
  "scripts": {
    "lint": "jshint src/**/*.js",
    "test": "karma start -c karma.conf.js",
    "prebuild": "rimraf dist/*",
    "build": "webpack --config webpack.production.config.js && node-sass scss/themes/default/proteic.scss dist/proteic.css --output-style compressed && node-sass scss/themes/lighter/lighter.scss dist/lighter.css --output-style compressed",
    "docs": "esdoc -c esdoc.json",
    "start": "webpack-dev-server",
    "prepublish": "npm run build"
  },
  "keywords": [
    "d3",
    "chart",
    "data visualization"
  ],
  "author": {
    "name": "Treelogic S.L.",
    "url": "http://www.treelogic.com/"
  },
  "contributors": [
    {
      "name": "Nacho García Fernández",
      "email": "0xnacho@gmail.com",
      "url": "https://0xnacho.github.io/"
    },
    {
      "name": "Jorge Yagüe París",
      "email": "jorgeyp@gmail.com",
      "url": "http://www.jorgeyp.com/"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/proteus-h2020/proteic.git"
  },
  "bugs": {
    "url": "https://github.com/proteus-h2020/proteic/issues"
  },
  "dependencies": {
    "d3": "^4.7.1",
    "d3-svg-annotation": "github:jorgeyp/d3-annotation",
    "d3-svg-legend": "^2.22.0",
    "rxjs": "^5.1.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/d3": "^4.3.0",
    "@types/mocha": "^2.2.38",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.26.1",
    "esdoc": "^0.5.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "jshint": "^2.9.2",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-webpack": "^2.0.1",
    "live-server": "^1.1.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "parallelshell": "^2.0.0",
    "protractor": "^5.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.4.5",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.5",
    "uglify-js": "^2.7.4",
    "watch": "^1.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-strip": "^0.1.0"
  }
}
